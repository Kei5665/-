<div class="main user-show">
  <div class="container">
    <div class="user category">
      <img src="<%="/category_images/#{@category.image_name}"%>">

      <h2><%= @category.name %></h2>
      
    <% @category.posts.each do |post| %>
      <!-- 指定されたコードを貼り付けてください -->
      <div class="posts-index-item">
        <div class="post-left">
          <img src="<%= "/user_images/#{post.user.image_name}" %>">
          <p><%= link_to(post.user.name, "/users/#{post.user.id}") %></p>
        </div>
        <div class="post-right">
          <div>
            <P><%=  post.created_at.strftime('%m/%d %H:%M') %></P>
            <div class="d-flex">
              <p class="title"><%= link_to(post.content, "/posts/#{post.id}") %></p>
              <P class="number"><%= link_to(post.numb, "/posts/#{post.id}") %></P>
            </div>
          </div>
        </div>
      </div>      
    <!-- 以下の<% %>を使ってeach文のendを追加してください -->
    <% end %>
      
    </div>
  </div>
</div>